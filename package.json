{
  "name": "anthracite-app",
  "productName": "Anthracite",
  "version": "0.1.0",
  "description": "Desktop Agent Browser powered by browser-use",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:python && tsc && vite build && electron-builder --config electron-builder.json",
    "build:python": "cd backend && python3 build.py",
    "preview": "vite preview",
    "postinstall": "electron-rebuild -f -w better-sqlite3",
    "rebuild": "electron-rebuild -f -w better-sqlite3"
  },
  "keywords": [
    "electron",
    "react",
    "agent",
    "browser",
    "ai"
  ],
  "author": "Anthracite",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yttrium400/anthracite-browser.git"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.15.4",
